# Kosmos E2E Testing - Local Reasoning Model (DeepSeek-R1 8B)
# Speed: 6-7 tok/s | VRAM: 5-6 GB
# Best for: E2E tests, complex reasoning, validation

export LLM_PROVIDER=openai
export OPENAI_API_KEY=ollama
export OPENAI_BASE_URL=http://localhost:11434/v1
export OPENAI_MODEL=deepseek-r1:8b

# Test configuration
export KOSMOS_TEST_TIMEOUT=600
export KOSMOS_TEST_TIER=e2e
export KOSMOS_ARTIFACTS_DIR=./test_artifacts

# Model parameters (optional overrides)
export LLM_TEMPERATURE=0.3
export LLM_MAX_TOKENS=8192
