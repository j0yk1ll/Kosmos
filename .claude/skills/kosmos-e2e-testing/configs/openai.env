# Kosmos E2E Testing - OpenAI API
# Best for: Production validation, final testing

export LLM_PROVIDER=openai
export OPENAI_API_KEY=${OPENAI_API_KEY:-your-api-key-here}
export OPENAI_BASE_URL=https://api.openai.com/v1
export OPENAI_MODEL=gpt-4o

# Test configuration
export KOSMOS_TEST_TIMEOUT=900
export KOSMOS_TEST_TIER=full
export KOSMOS_ARTIFACTS_DIR=./test_artifacts

# Model parameters (optional overrides)
export LLM_TEMPERATURE=0.3
export LLM_MAX_TOKENS=8192
